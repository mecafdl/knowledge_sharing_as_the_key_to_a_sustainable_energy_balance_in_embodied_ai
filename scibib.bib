  keywords  = {humanoid robots;matrix algebra;robot dynamics;robot kinematics;trajectory control;Cartesian space;dynamic movement primitives;DMPs;robot behaviors;rotation matrices;quaternions;phase stopping mechanism;orientation trajectory;humanoid robot;Quaternions;Trajectory;Robots;Equations;Angular velocity;Mathematical model;Differential equations},
  owner     = {johannes},

@article{Averta2020,
doi= {10.1093/gigascience/giab043},
    author = {Averta, Giuseppe and Barontini, Federica and Catrambone, Vincenzo and Haddadin, Sami and Handjaras, Giacomo and Held, Jeremia P O and Hu, Tingli and Jakubowitz, Eike and Kanzler, Christoph M and Kühn, Johannes and Lambercy, Olivier and Leo, Andrea and Obermeier, Alina and Ricciardi, Emiliano and Schwarz, Anne and Valenza, Gaetano and Bicchi, Antonio and Bianchi, Matteo},
    title = "{U-Limb: A multi-modal, multi-center database on arm motion control in healthy and post-stroke conditions}",
    journal = {GigaScience},
    volume = {10},
    number = {6},
    year = {2021},
    month = {06},
    abstract = "{Shedding light on the neuroscientific mechanisms of human upper limb motor control, in both healthy and disease conditions (e.g., after a stroke), can help to devise effective tools for a quantitative evaluation of the impaired conditions, and to properly inform the rehabilitative process. Furthermore, the design and control of mechatronic devices can also benefit from such neuroscientific outcomes, with important implications for assistive and rehabilitation robotics and advanced human-machine interaction. To reach these goals, we believe that an exhaustive data collection on human behavior is a mandatory step. For this reason, we release U-Limb, a large, multi-modal, multi-center data collection on human upper limb movements, with the aim of fostering trans-disciplinary cross-fertilization.This collection of signals consists of data from 91 able-bodied and 65 post-stroke participants and is organized at 3 levels: (i) upper limb daily living activities, during which kinematic and physiological signals (electromyography, electro-encephalography, and electrocardiography) were recorded; (ii) force-kinematic behavior during precise manipulation tasks with a haptic device; and (iii) brain activity during hand control using functional magnetic resonance imaging.}",
    issn = {2047-217X},
    doi = {10.1093/gigascience/giab043},
    url = {https://doi.org/10.1093/gigascience/giab043},
    note = {giab043},
    eprint = {https://academic.oup.com/gigascience/article-pdf/10/6/giab043/38694241/giab043.pdf},
}
